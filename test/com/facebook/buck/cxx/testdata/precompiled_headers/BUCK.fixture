cxx_library(
    name = "some_library",
    srcs = [
        "lib.c",
    ],
    exported_headers = [
        "lib.h",
    ],
    headers = [
        "referenced_by_prefix_header.h",
    ],
    prefix_header = "prefix_header.h",
    deps = [
        ":some_dependency",
    ],
)

cxx_library(
    name = "some_dependency",
    srcs = [
        "some_dependency.c",
    ],
    exported_headers = [
        "referenced_by_prefix_header_from_dependency.h",
    ],
)

cxx_library(
    name = "library_multicell_pch",
    srcs = [
        "LibFileUsingMulticell1.c",
        "LibFileUsingMulticell2.c",
    ],
    precompiled_header = "secondary//pch:pch",
)

cxx_binary(
    name = "some_binary",
    srcs = [
        "bin.c",
    ],
    deps = [
        ":some_library",
    ],
)
